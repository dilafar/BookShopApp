<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/parent_layout"
    android:layout_width="match_parent"
    android:layout_height="145dp"
    android:padding="10dp">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/image22"
        android:layout_width="103dp"
        android:layout_height="110dp"
        android:src="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/categoryname20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_toRightOf="@id/image22"
        android:text="Product Name"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/categoryname20"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image22"
        android:text="Description"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_name2"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image22"
        android:text="Av.Qty"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="56dp"
        android:layout_toRightOf="@id/image_name3"
        android:text="Qty"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_name3"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image22"
        android:text="Price"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="85dp"
        android:layout_toRightOf="@id/image_name5"
        android:text="15"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="250dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="85dp"
        android:text="%"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="85dp"
        android:text="Rs"
        android:textColor="#000000"
        android:textSize="17sp" />


</RelativeLayout>
