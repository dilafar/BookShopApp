<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent_layout2"
    android:layout_width="match_parent"
    android:layout_height="145dp"
    android:padding="10dp">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/image23"
        android:layout_width="103dp"
        android:layout_height="110dp"
        android:src="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/categoryname23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_toRightOf="@id/image23"
        android:text="Product Name"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/categoryname23"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image23"
        android:text="Description"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_name23"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image23"
        android:text="Av.Qty"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="56dp"
        android:layout_toRightOf="@id/image_name33"
        android:text="Qty"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_name33"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/image23"
        android:text="Price"
        android:textColor="#000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/image_name63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="85dp"
        android:layout_toRightOf="@id/image_name53"
        android:text="15"
        android:textColor="#000"
        android:textSize="17sp" />

    <ImageView
        android:id="@+id/updateitem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="310dp"
        android:layout_marginLeft="310dp"
        android:layout_marginTop="85dp"
        app:srcCompat="@drawable/ic_baseline_update_24"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/deleteitem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="340dp"
        android:layout_marginLeft="340dp"
        android:layout_marginTop="85dp"
        app:srcCompat="@drawable/ic_baseline_delete_24"
        tools:ignore="VectorDrawableCompat" />


</RelativeLayout>
